{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "picking_app-config1",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "fiori",
        "run"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "args": [
        "--open",
        "index.html"
      ],
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "outputCapture": "std",
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/picking_app\"}",
        "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/sap\",\"url\":\"http://ds4cloudfoundry:443\",\"destination\":\"DS4_BasicAuth\"}]",
        "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false&sap-client=110"
      }
    }
  ]
}